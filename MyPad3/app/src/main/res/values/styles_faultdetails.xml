<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="table_th">
        <item name="android:paddingTop">7.333dp</item>
        <item name="android:paddingBottom">7.333dp</item>
        <item name="android:paddingStart">32.667dp</item>
        <item name="android:paddingEnd">32.667dp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">10.667sp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="table_td">
        <item name="android:paddingTop">9.333dp</item>
        <item name="android:paddingBottom">9.333dp</item>
        <item name="android:paddingStart">32.667dp</item>
        <item name="android:paddingEnd">32.667dp</item>
        <item name="android:textColor">#ffff03</item>
        <item name="android:textSize">10.667sp</item>
        <item name="android:gravity">center</item>
